            "command": {
                "arguments": [],
                "program": ""
            },
            "system": "auto"
        },
        "tokenizer": "regex",
        "unknown_words": {
            "fail_when_present": true,
            "guess_pronunciations": true
        }
    },
    "wake": {
        "command": {
            "arguments": [],
            "program": ""
        },
