            "chunk_delay": 0.005,
            "chunk_size": 2048,
            "engine_path": "precise-engine",
            "model": "hey-mycroft-2.pb",
            "sensitivity": 0.5,
            "trigger_level": 3
        },
        "snowboy": {
            "apply_frontend": false,
            "audio_gain": 1,
            "chunk_size": 960,
            "model": "snowboy.umdl",
